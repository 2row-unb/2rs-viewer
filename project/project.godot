; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="2RS Viewer"
run/main_scene="res://screens/Main.tscn"
config/icon="res://resources/icon.png"

[debug]

settings/fps/force_fps=10

[display]

window/size/borderless=true

[rendering]

quality/intended_usage/framebuffer_allocation=3
quality/intended_usage/framebuffer_allocation.mobile=2
environment/default_clear_color=Color( 0.298039, 0.298039, 0.298039, 1 )
